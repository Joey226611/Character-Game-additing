coroutine.wrap(QCYGAQ_fake_script)()
local function ICXVQHK_fake_script() -- BackGround.DraggableFrame 
	local script = Instance.new('LocalScript', BackGround)

	local frame = script.Parent
	local dragging = false
	local dragInput, mousePos, framePos
	
	local function update(input)
	    local delta = input.Position - mousePos
	    frame.Position = UDim2.new(
	        framePos.X.Scale,
	        framePos.X.Offset + delta.X,
	        framePos.Y.Scale,
	        framePos.Y.Offset + delta.Y
	    )
	end
	
	frame.InputBegan:Connect(function(input)
	    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
	        dragging = true
	        mousePos = input.Position
	        framePos = frame.Position
	
	        input.Changed:Connect(function()
	            if input.UserInputState == Enum.UserInputState.End then
	                dragging = false
	            end
	        end)
	    end
	end)
	
	frame.InputChanged:Connect(function(input)
	    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
	        dragInput = input
	    end
	end)
	
	game:GetService("UserInputService").InputChanged:Connect(function(input)
	    if dragging and input == dragInput then
	        update(input)
	    end
	end)
	
	
end
coroutine.wrap(ICXVQHK_fake_script)()
local function BSWYG_fake_script() -- CM_2.Script 
	local script = Instance.new('Script', CM_2)

	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
		local parent = button.Parent
		while parent and not parent:IsA("ScreenGui") do
			parent = parent.Parent
		end
	
		if parent and parent.Name == "RG//E" then
			local scrollingFrame3 = parent:FindFirstChild("ScrollingFrame3")
			local scrollingFrame1 = parent:FindFirstChild("ScrollingFrame1")
	
			if scrollingFrame3 and scrollingFrame3:IsA("Frame") then
				scrollingFrame3.Visible = false
			end
	
			if scrollingFrame1 and scrollingFrame1:IsA("Frame") then
				scrollingFrame1.Visible = true
			end
		end
	end)
	
	
end
coroutine.wrap(BSWYG_fake_script)()
local function PWYEFD_fake_script() -- HavenMP.ToggleHavenSoundClient 
	local script = Instance.new('LocalScript', HavenMP)

	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
	    local sound = game.Workspace:FindFirstChild("HAVEN")
	    if sound and sound:IsA("Sound") then
	        sound.IsPlaying = not sound.IsPlaying
	    end
	end)
	
	
end
coroutine.wrap(PWYEFD_fake_script)()
local function JWIV_fake_script() -- LMMP.ToggleHavenSoundClient 
	local script = Instance.new('LocalScript', LMMP)

	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
		local sound = game.Workspace:FindFirstChild("lugic mansion")
	    if sound and sound:IsA("Sound") then
	        sound.IsPlaying = not sound.IsPlaying
	    end
	end)
	
	
end
coroutine.wrap(JWIV_fake_script)()
