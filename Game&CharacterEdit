-- Scripts:

local function KLRFMPL_fake_script() -- SpeedChancer.SpeedChangerClient 
	local script = Instance.new('LocalScript', SpeedChancer)

	local textBox = script.Parent
	
	textBox.FocusLost:Connect(function(enterPressed)
	    if enterPressed then
	        local input = tonumber(textBox.Text)
	        if input then
	            local player = game.Players.LocalPlayer
	            local character = player.Character or player.CharacterAdded:Wait()
	            local humanoid = character:FindFirstChildOfClass("Humanoid")
	            if humanoid then
	                humanoid.WalkSpeed = input
	            end
	        end
	    end
	end)
	
	
end
coroutine.wrap(KLRFMPL_fake_script)()
local function AWCW_fake_script() -- JumppowerChancer.JumpPowerChangerClient 
	local script = Instance.new('LocalScript', JumppowerChancer)

	local textBox = script.Parent
	
	textBox.FocusLost:Connect(function(enterPressed)
	    if enterPressed then
	        local input = tonumber(textBox.Text)
	        if input then
	            local player = game.Players.LocalPlayer
	            local character = player.Character or player.CharacterAdded:Wait()
	            local humanoid = character:FindFirstChildOfClass("Humanoid")
	            if humanoid then
	                humanoid.JumpPower = input
	            end
	        end
	    end
	end)
	
	
end
coroutine.wrap(AWCW_fake_script)()
local function NCSKA_fake_script() -- NB.NightButtonClient 
	local script = Instance.new('LocalScript', NB)

	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
	    game.Lighting.TimeOfDay = "00:00:00"
	end)
	
	
end
